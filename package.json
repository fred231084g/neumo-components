{
  "name": "@obinexuscomputing/neumo-components",
  "version": "1.0.0",
  "description": "Neumorphism SVG components library",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/components/index.js",
  "types": "dist/types/components/index.d.ts",
  "style": "dist/css/index.css",
  
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/components/index.d.ts",
      "import": "./dist/esm/components/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/umd/index.js"
    },
    "./styles": "./dist/css/index.css"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "web-dev-server --node-resolve --open --watch",
    "serve": "web-dev-server --root-dir dist --node-resolve --open --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/postcss-import": "^14.0.3",
    "@types/postcss-preset-env": "^7.7.0",
    "@web/dev-server": "^0.4.1",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.2",
    "postcss-scss": "^4.0.9",
    "rollup": "^4.9.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.83.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  }
}
